                      description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  bo.boor_user_id , f.faci_hotel_id,
		CONVERT(INT, CONVERT(VARCHAR, bo.boor_order_date, 112)) AS tiempo_id,
		 
		 bo.boor_total_guest as Huesped,
		  bo.boor_down_payment as Pagos_anticipados,
		sum(bod.borde_discount )as MontoTotalDescuentos
FROM Hotel_Realta.Booking.booking_orders bo
INNER JOIN Hotel_Realta.Booking.booking_order_detail bod 
ON bo.boor_id = bod.borde_boor_id
LEFT JOIN Hotel_Realta.Hotel.Facilities f 
ON bod.borde_faci_id = f.faci_id
LEFT JOIN Hotel_Realta.Hotel.Hotels h
ON f.faci_hotel_id = h.hotel_id
left join Hotel_Realta.Users.users  as u on u.user_id=bo.boor_user_id
--where bo.boor_user_id NOT IN (SELECT id_user FROM Hotel_RealtaDM.Reserva.FactReservasR)
--	AND f.faci_hotel_id NOT IN (SELECT id_hotel FROM Hotel_RealtaDM.Reserva.FactReservasR)
--	AND bod.borde_id NOT IN (SELECT id_promocion FROM Hotel_RealtaDM.Reserva.FactReservasR)
--	AND CONVERT(INT, CONVERT(VARCHAR, bo.boor_order_date, 112)) NOT IN (SELECT id_fecha FROM Hotel_RealtaDM.Reserva.DimTiempoR)
GROUP BY
	bod.borde_id, bo.boor_user_id,faci_hotel_id, CONVERT(INT, CONVERT(VARCHAR, bo.boor_order_date, 112)),boor_total_guest,  bo.boor_down_payment</property>

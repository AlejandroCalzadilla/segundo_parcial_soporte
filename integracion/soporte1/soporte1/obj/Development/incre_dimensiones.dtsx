                      dataType="System.String"
                      description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
 h.hotel_name as hotel ,
 sec.sec_name as nombre_area,
 ty.roomtp_name as tipo_habitacion,

 CONCAT( h.hotel_id,
 ha.room_id,
 sec.sec_id ) as numero_habitacion


FROM  Hotel.hotels AS h
left join dbo.sector as sec on sec.sec_hotel_id=h.hotel_id 
left join hotel.Facilities as fa on fa.faci_hotel_id= h.hotel_id
left join dbo.Room as ha on fa.faci_room_id=ha.room_id
left join dbo.Room_type as ty on ty.roomtp_id=ha.room_rotp_id
WHERE NOT EXISTS (
    SELECT 1
    FROM Hotel_RealtaE.dbo.td   fac
    WHERE  numero_habitacion= fac.numero_habitacion
)</property>
